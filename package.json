{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:migrate": "turbo run migrate --filter=db",
    "db:update": "turbo run update --filter=db",
    "web:codegen": "turbo run codegen --filter=web"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "@repo/eslint-config": "*",
    "prettier": "^3.2.5",
    "turbo": "latest",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
